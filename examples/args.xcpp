#if !defined(__XCPP__)
#define __XCPP__ 0
#elif defined(__XCPP__)
#pragma once
#else
. xcpp.sh "$0" "$@"
#endif

// Prints all command arguments
int args( strings arguments )
{
    for( size i{ 0 }; i < arguments.size(); ++i )
    {
		cout << "args[" << i << "] " << arguments[i] << endl;
    }
    return 0;
}
